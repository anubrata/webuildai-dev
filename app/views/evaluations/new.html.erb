<div class="row">
 <div class = "login-b1">
   <div>
     <div>
       <span>Email</span>
         <br /> <br /> <br /> <hr id = 'hhr'> <br />
         <%= form_with url: sessions_path, local: true do |f| %>
         <div class="control-group-id">
           <%= f.label :email, 'Email' %>
           <div class="controls">
             <%= f.text_field :id, placeholder: "Email" %>
           </div>
         </div>
         <br/>
         <div class="control-group-password">
           <%= f.label :name %>
           <div class="controls">
             <%= f.text_field :name, placeholder: "Name" %>
           </div>
         </div>
         <div class="actions-login"><%= f.submit "Submit" %></div>
       <% end %>
     </div>
   </div>
 </div>
</div>
